{
  "version": 2,
  "builds": [
    {
      "src": "website/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "website/pages/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/website/src/index.js"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/website/pages/$1"
    },
    {
      "src": "/(.+)",
      "dest": "/website/src/index.js"
    }
  ]
}
