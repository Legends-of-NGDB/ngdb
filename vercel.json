{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "website/pages/**/*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/website/pages/ngdb.html" },
    { "src": "/pages/(.*)", "dest": "/website/pages/$1" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
